<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/forest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.android.user.thespellfactory10.Level4">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/stone1"
        android:onClick="Stone1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="187dp"
        android:layout_marginStart="187dp"
        android:layout_marginBottom="44dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/stone3"
        android:onClick="Stone3"
        android:visibility="invisible"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/stone1"
        android:layout_toEndOf="@+id/stone1"
        android:layout_marginTop="59dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:visibility="invisible"
        android:id="@+id/stone2"
        android:onClick="Stone2"
        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp"
        android:layout_above="@+id/stone1"
        android:layout_toRightOf="@+id/stone1"
        android:layout_toEndOf="@+id/stone1" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:onClick="Stone4"
        android:visibility="invisible"
        android:id="@+id/stone4"
        android:layout_alignTop="@+id/stone3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="37dp"
        android:layout_marginEnd="37dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/stone5"
        android:onClick="Stone5"
        android:visibility="invisible"
        android:layout_alignTop="@+id/stone3"
        android:layout_toLeftOf="@+id/stone3"
        android:layout_toStartOf="@+id/stone3" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/stone6"
        android:onClick="Stone6"
        android:visibility="invisible"
        android:layout_above="@+id/stone2"
        android:layout_toRightOf="@+id/stone2"
        android:layout_toEndOf="@+id/stone2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/stone7"
        android:onClick="Stone7"
        android:visibility="invisible"
        android:layout_alignTop="@+id/stone1"
        android:layout_alignRight="@+id/stone2"
        android:layout_alignEnd="@+id/stone2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/stone8"
        android:onClick="Stone8"
        android:visibility="invisible"
        android:layout_above="@+id/stone2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/stone9"
        android:onClick="Stone9"
        android:visibility="invisible"
        android:layout_alignBottom="@+id/stone7"
        android:layout_alignRight="@+id/stone4"
        android:layout_alignEnd="@+id/stone4" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="\?"
        android:id="@+id/button16"
        android:onClick="Help"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
